{
	"name": "chrome-extension-getmetainfo",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "chrome-extension-getmetainfo",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
		"browser_action": {
	    "default_icon": "icon16.png",
	    "default_title": "popup",
	    "default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}